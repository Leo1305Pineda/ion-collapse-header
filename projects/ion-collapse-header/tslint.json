{
  "extends": "../../tslint.json",
  "rules": {
    "use-host-property-decorator": false,
    "no-host-metadata-property": false,
    "directive-selector": [
      false,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case"
    ]
  }
}
